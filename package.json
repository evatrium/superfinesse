{
	"name": "superfinesse",
	"version": "0.0.3",
	"description": "modified version of superfine",
	"main": "lib/index.js",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src",
		"lib"
	],
	"keywords": [
		"vdom for web components",
		"custom elements",
		"superfinesse",
		"superfine",
		"frontend",
		"virtual dom",
		"vdom"
	],
	"license": "MIT",
	"scripts": {
		"start": "rollup -c -w --environment project:demo",
		"build": "rollup -c --environment preset:lib",
		"test": "exit 0"
	},
	"rollupConfig": {
		"project": {
			"demo": {
				"preset": "dev",
				"input": "demo/src/index.js",
				"html": "demo/src/index.html",
				"output": "demo/build"
			}
		}
	},
	"devDependencies": {
		"rollup-configured": "^0.0.13"
	}
}
